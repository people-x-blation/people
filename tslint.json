{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended","tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import |^export {(.*?)}",
        "check-strings": true,
        "check-regex": true
      }
    ],
    "quotemark": [true, "single"],
    "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}